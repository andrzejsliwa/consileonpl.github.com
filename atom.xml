---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Consileon Dev Blog</title>
 <link href="http://consileonpl.github.com/atom.xml" rel="self"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://consileonpl.github.com/</id>
 {% for post in site.posts limit:10 %}
 <entry>
   <id>http://consileonpl.github.com{{ post.id }}</id>
   <link href="http://consileonpl.github.com{{ post.url }}"/>
   <title>{{ post.title }}</title>
   <content type="html">{{ post.content | xml_escape }}</content>{% for tag in post.categories %}
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <category scheme="http://consileonpl.github.com/" term="{{ tag }}" label="{{ tag }}" />{% endfor %}
 </entry>
 {% endfor %}
</feed>